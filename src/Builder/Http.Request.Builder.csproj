<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Http Request Builder</Title>
    <Authors>Reza Ahmadi</Authors>
    <Version>8.0.0</Version>
    <Description>Http request as builder pattern for easier to use.
You can send request and deserialize response of request.</Description>
    <Copyright>Copyright Reza Ahmadi</Copyright>
    <PackageProjectUrl>https://github.com/RezaAmd/Http.Request.Builder</PackageProjectUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/RezaAmd/Http.Request.Builder</RepositoryUrl>
    <PackageTags>Http, Request, response, Builder Pattern</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageReadmeFile>Http request as builder pattern for.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="C:\Users\Reza\Desktop\Http request as builder pattern for.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
